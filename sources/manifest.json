{
	"name": "Search Moar!",
	"version": "1.0",
	"description": "Switch between Yandex and Google SERP with a single click.",
	"background": { "scripts": ["background.js"] },
	"page_action": {
		"default_title": "Search moar!",
		"default_icon" : "icons/icon-18.png"
	},
	"permissions": [
		"tabs",
		"http://*.google.com/",
		"http://*.google.ru/",
		"http://*.yandex.ru/"
	],
	"icons": {
		"16" : "icons/icon-16.png",
		"48" : "icons/icon-48.png",
		"128" : "icons/icon-128.png"
	}
}